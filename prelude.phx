true: 0 0 = ;
false: 0 1 = ;

drop: pop ;

keep: over 'call dip ;
bi: 'keep dip call ;
tri: ('keep dip keep) dip call ;

2curry: curry curry ;
with: swapd (swapd call) 2curry ;
cleave: 'call with each ;

2dip: swap 'dip dip ;
2keep: '2dup dip 2dip ;
2bi: '2keep dip call ;

+-: '+ '- 2bi ;

part: ['f] <- (f select) (f reject) bi ;
